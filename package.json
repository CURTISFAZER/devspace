{
  "name": "devspace",
  "version": "1.0.0-beta.5",
  "main": "index.js",
  "dependencies": {
    "elemental": "0.5.13",
    "firebase": "^2.3.1",
    "lodash.indexof": "^4.0.1",
    "lodash.words": "^4.0.0",
    "octicons": "^3.1.0",
    "parse-link-header": "^0.4.1",
    "re-base": "bfollington/re-base#master",
    "react": "0.14.8",
    "react-addons-css-transition-group": "^0.14.3",
    "react-addons-update": "^0.14.6",
    "react-dom": "0.14.8",
    "react-mixin": "^2.0.2",
    "react-timeago": "^2.2.1",
    "react-timer-mixin": "^0.13.3",
    "superagent": "^1.6.1",
    "superagent-throttle": "0.0.5"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "critical": "^0.7.2",
    "del": "^2.2.0",
    "electron-builder": "^3.1.1",
    "electron-debug": "^0.5.1",
    "electron-packager": "^6.0.0",
    "electron-prebuilt": "^0.37.3",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.0",
    "gulp-connect": "^3.2.0",
    "gulp-less": "3.0.5",
    "gulp-load-plugins": "^1.2.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-minify-html": "^1.0.6",
    "gulp-notify": "2.2.0",
    "gulp-rename": "1.2.2",
    "gulp-shell": "^0.5.2",
    "gulp-uglify": "1.5.3",
    "gulp-util": "3.0.7",
    "run-sequence": "^1.1.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.6.1"
  },
  "scripts": {
    "start": "electron .",
    "clean": "rm -rf build",
    "clean:osx": "rm -rf build/osx",
    "clean:win": "rm -rf build/win",
    "pack": "npm run clean && npm run pack:osx && npm run pack:win",
    "pack:osx": "npm run clean:osx && electron-packager . DevSpace --out=build/osx --platform=darwin --arch=x64  --version=0.37.3 --icon=src/images/osx/icon.icns --app-version=\"1.0.0-beta.5\" --ignore=\"node_modules|src|dist|build\"",
    "pack:win": "npm run clean:win && electron-packager . DevSpace --out=build/win --platform=win32  --arch=ia32 --version=0.37.3 --icon=src/images/win/icon.ico  --app-version=\"1.0.0-beta.5\" --ignore=\"node_modules|src|dist|build\"",
    "build": "npm run build:osx && npm run build:win",
    "build:osx": "npm run pack:osx && electron-builder \"build/osx/DevSpace-darwin-x64/DevSpace.app\"   --platform=osx --out=\"build/osx\" --config=builder.json",
    "build:win": "npm run pack:win && electron-builder \"build/win/DevSpace-win32-ia32\" --platform=win --out=\"build/win\" --config=builder.json"
  }
}
